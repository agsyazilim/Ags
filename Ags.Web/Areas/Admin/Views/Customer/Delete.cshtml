@model AdminRegisterViewModel
@{
    ViewData["Title"] = "Delete";
}

<form asp-action="Delete" class="form-horizontal">
    <div asp-validation-summary="ModelOnly" class="text-danger"></div>
    <div class="content-header clearfix">
        <h1 class="pull-left">
            Yeni ekle
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="Index">Listeye Dön</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-danger">
                <i class="fa fa-floppy-o"></i>
               Sil
            </button>
        </div>
    </div>
<div class="content">
    <div class="panel-group">
        <div class="panel panel-default">
            <div class="panel-body">

                <div class="form-group">
                    <div class="col-md-3">
                        <ags-label asp-for="FirstName" />
                    </div>
                    <div class="col-md-9">
                        <ags-editor asp-for="FirstName" />
                        <span asp-validation-for="FirstName" class="text-danger"></span>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-md-3">
                        <ags-label asp-for="LastName" />
                    </div>
                    <div class="col-md-9">
                        <ags-editor asp-for="LastName"  />
                        <span asp-validation-for="LastName" class="text-danger"></span>
                    </div>
                </div>

                <div class="form-group">
                    <div class="col-md-3">
                        <ags-label asp-for="Email" />
                    </div>
                    <div class="col-md-9">
                        <ags-editor asp-for="Email" />
                        <span asp-validation-for="Email" class="text-danger"></span>
                    </div>
                </div>



            </div>
        </div>
    </div>
</div>
</form>



