@model TopicModel

<div asp-validation-summary="All"></div>
<input asp-for="Id" type="hidden" />

<div class="content">
    <div class="form-horizontal">

        <ags-tabs id="topic-edit">
            <ags-tab asp-name="tab-info" asp-default="true" asp-title="Ayarlar">@await Html.PartialAsync("_CreateOrUpdate.Info", Model)</ags-tab>
            <ags-tab asp-name="tab-seo" class="advanced-setting" asp-title="SEO">@await Html.PartialAsync("_CreateOrUpdate.SEO", Model)</ags-tab>
        </ags-tabs>
    </div>
</div>

