@model EmailAccountModel

@{
    //page title
    ViewBag.Title = "EditEmailAccountDetails";
    //active menu item (system name)
}

<form asp-controller="EmailAccount" asp-action="Edit" method="post">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            Düzenle
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="List">Liste Geri Dön</a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                Kaydet
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
               Kaydet Devam Et
            </button>
            <span id="emailAccount-delete" class="btn bg-red">
                <i class="fa fa-trash-o"></i>
                Sil
            </span>
                          </div>
    </div>
    @await Html.PartialAsync("_CreateOrUpdate", Model)
</form>
<ags-delete-confirmation asp-model-id="@Model.Id" asp-button-id="emailAccount-delete" />
